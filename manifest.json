{
  "name": "LeagueOfFitness",
  "version": "1.0",
  "manifest_version": 2,
  "description": "https://www.reddit.com/r/leagueoflegends/comments/1li068/league_of_fitness/",
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "img/logo.png",
    "default_popup": "popup.html",
    "default_title": "League Of Fitness"
  },
  "icons": {
    "48": "img/logo.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>"
  ]
}